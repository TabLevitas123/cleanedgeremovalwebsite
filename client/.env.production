# Clean Edge Removal Website - Production Environment Variables

# Application
REACT_APP_NAME=Clean Edge Removal
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=Professional junk removal and property cleanout services

# API Configuration
REACT_APP_API_URL=https://api.cleanedgeremoval.com/api
REACT_APP_API_TIMEOUT=15000

# Authentication
REACT_APP_AUTH_COOKIE_NAME=clean_edge_auth
REACT_APP_AUTH_COOKIE_SECURE=true
REACT_APP_AUTH_COOKIE_DOMAIN=cleanedgeremoval.com
REACT_APP_AUTH_TOKEN_EXPIRY=86400

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_FEATURE_DARK_MODE=true
REACT_APP_FEATURE_ONLINE_BOOKING=true
REACT_APP_FEATURE_LIVE_CHAT=true
REACT_APP_FEATURE_CUSTOMER_PORTAL=true

# Logging
REACT_APP_LOG_LEVEL=info
REACT_APP_REMOTE_LOGGING_URL=https://logging.cleanedgeremoval.com/api/logs

# IONOS Integration
REACT_APP_IONOS_API_URL=https://api.ionos.com
# IMPORTANT: These values should be set during the build process
REACT_APP_IONOS_API_KEY=
REACT_APP_IONOS_API_SECRET=

# Email Service
REACT_APP_EMAIL_SERVICE_URL=https://api.cleanedgeremoval.com/api/email
REACT_APP_EMAIL_FROM_ADDRESS=noreply@cleanedgeremoval.com
REACT_APP_EMAIL_CONTACT_ADDRESS=contact@cleanedgeremoval.com

# Google Maps API (for location services)
# IMPORTANT: This value should be set during the build process
REACT_APP_GOOGLE_MAPS_API_KEY=

# Stripe Integration (for payments)
# IMPORTANT: These values should be set during the build process
REACT_APP_STRIPE_PUBLIC_KEY=
REACT_APP_STRIPE_PRICE_ID_BASIC=
REACT_APP_STRIPE_PRICE_ID_STANDARD=
REACT_APP_STRIPE_PRICE_ID_PREMIUM=

# Social Media
REACT_APP_SOCIAL_FACEBOOK=https://facebook.com/cleanedgeremoval
REACT_APP_SOCIAL_TWITTER=https://twitter.com/cleanedgeremoval
REACT_APP_SOCIAL_INSTAGRAM=https://instagram.com/cleanedgeremoval
REACT_APP_SOCIAL_LINKEDIN=https://linkedin.com/company/cleanedgeremoval

# Contact Information
REACT_APP_CONTACT_PHONE=(800) 555-1234
REACT_APP_CONTACT_EMAIL=info@cleanedgeremoval.com
REACT_APP_CONTACT_ADDRESS=123 Main St, South Bend, IN 46601

# Business Hours
REACT_APP_BUSINESS_HOURS_MON_FRI=8:00 AM - 6:00 PM
REACT_APP_BUSINESS_HOURS_SAT=9:00 AM - 4:00 PM
REACT_APP_BUSINESS_HOURS_SUN=Closed

# Service Areas (comma-separated list)
REACT_APP_SERVICE_AREAS=South Bend,Mishawaka,Elkhart,Granger,Goshen,Plymouth,Warsaw,Michigan City,La Porte,Valparaiso

# Performance Monitoring
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_PERFORMANCE_SAMPLE_RATE=0.1

# Error Tracking
REACT_APP_ENABLE_ERROR_TRACKING=true
REACT_APP_ERROR_SAMPLE_RATE=1.0

# Content Delivery Network (CDN)
REACT_APP_CDN_URL=https://cdn.cleanedgeremoval.com

# Maintenance Mode
REACT_APP_MAINTENANCE_MODE=false
REACT_APP_MAINTENANCE_END_TIME=
REACT_APP_MAINTENANCE_MESSAGE=

# Security
REACT_APP_ENABLE_CSP=true
REACT_APP_ENABLE_HSTS=true
REACT_APP_ENABLE_XSS_PROTECTION=true
REACT_APP_ENABLE_CONTENT_TYPE_OPTIONS=true
REACT_APP_ENABLE_FRAME_OPTIONS=true

# Build Optimization
REACT_APP_GENERATE_SOURCEMAP=false
REACT_APP_INLINE_RUNTIME_CHUNK=true
REACT_APP_MINIMIZE=true
REACT_APP_COMPRESS=true